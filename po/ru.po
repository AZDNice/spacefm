#: ../src/settings.c:9208
#, c-format
msgid ""
"Enter device display name format:\n"
"\n"
"Use:\n"
"\t%%v\tdevice filename (eg sdd1)\n"
"\t%%s\ttotal size (eg 800G)\n"
"\t%%t\tfstype (eg ext4)\n"
"\t%%l\tvolume label (eg Label or [no media])\n"
"\t%%m\tmount point if mounted, or ---\n"
"\t%%i\tdevice ID\n"
msgstr "Введите формат отображения устройства:\n\nПодстановка:\n\t%%v\tимя файла устройства (например, sdd1)\n\t%%s\tобщий размер (например, 800G)\n\t%%t\tтип файловой системы (например, ext4)\n\t%%l\tметка раздела (например, Label или [нет носителя])\n\t%%m\tточка монтирования или --- если не примонтировано\n\t%%i\tID устройства\n"

